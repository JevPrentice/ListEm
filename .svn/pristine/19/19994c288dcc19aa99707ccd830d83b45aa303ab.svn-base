<?xml version="1.0" encoding="UTF-8"?>
<ui xmlns="http://uiprogram.mezzanine.com/View" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://uiprogram.mezzanine.com/View ../../xsd/View.xsd">
    <view label="average_cu_perfomance.title" unit="AverageCUProgress" init="init">

        <navigation outcome="navCUPerformance" target="CUPerformance" />
        <action label="back" action="back"/>

        <table title="PerformanceSummary.OverviewSummary">
            <collectionSource variable="results"/>
            <column heading="topic">
                <attributeName>topicid</attributeName>
            </column>        
            <column heading="PerformanceSummary.averageGroupProgress">
                <attributeName>averageprogress</attributeName>
            </column>
            <column heading="PerformanceSummary.averagetotalscore">
                <attributeName>averagetotalscore</attributeName>
            </column>
        </table>

    </view>
</ui>