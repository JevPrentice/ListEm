@Restrict("SmartChew", all())
persistent object Sequence{
	@RequiredAttribute("clientGroup.requirefield")
	string sequenceID;
	@RequiredAttribute("clientGroup.requirefield")
	string sequenceName;
	string sequenceStartSMS;
	string sequenceEndSMS;
	string sequenceReminderSMS;
    @RequiredAttribute("clientGroup.requirefield")
	int sequenceReminderTime;
}
