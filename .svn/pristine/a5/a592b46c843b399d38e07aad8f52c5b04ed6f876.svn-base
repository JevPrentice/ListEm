@Role("Admin")
persistent object Admin {

	@RequiredAttribute("error.required_field.first_name")
	string firstNames;
	@RequiredAttribute("error.required_field.last_name")
	string surname;
	@InternationalNumber("client.requireInternationalNumber")
	string mobileNumber;

	datetime registered;
}