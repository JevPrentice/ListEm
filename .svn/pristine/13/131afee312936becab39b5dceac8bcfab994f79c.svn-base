<?xml version="1.0" encoding="UTF-8"?>
<ui xmlns="http://uiprogram.mezzanine.com/View" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://uiprogram.mezzanine.com/View ../views/xsd/View.xsd">
	<view label="group_bulk_upload.title" unit="GroupBulkUpload" init="init" >

        <navigation outcome="nav_group_management" target="GroupManagement"/>

        <action label="back" action="back"/>

        <info label="group_bulk_upload.csv_template_header">
            <binding variable="csv_template_header">
            </binding>
        </info>

         <info label="group_bulk_upload.csv_template_content">
            <binding variable="csv_template_content">
            </binding>
        </info>

        <select label="client_group.assing_chew">
            <binding variable="selected_chew">
                 <!--<attribute name="client_chews"/>-->
            </binding>
           <collectionSource function="ClientList:getClients">
                <displayAttribute name="firstName"/>
                <displayAttribute name="surname"/>
                <displayAttribute name="mobileNumber"/>
            </collectionSource>
        </select>
 
        <fileupload label="group_bulk_upload.fileupload">
            <binding variable="uploadedFile">
                <attribute name="data" />
            </binding>
        </fileupload>
       
        <submit label="save" action="processCSV"/>

	</view>
</ui>