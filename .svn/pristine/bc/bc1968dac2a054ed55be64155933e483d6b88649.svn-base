<?xml version="1.0" encoding="UTF-8"?>
<ui xmlns="http://uiprogram.mezzanine.com/View" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://uiprogram.mezzanine.com/View ../views/xsd/View.xsd">

    <view label="invite.learningManager" unit="RoleManagement" init="initInviteLM">

        <navigation outcome="goBack"    target="ManageLearningManagers"/>
        <navigation outcome="error"    target="InviteLearningManager"/>
        <action label="back" outcome="goBack"/>

        <textfield label="firstNames">
            <binding variable="lm">
                <attribute name="firstNames"/>
            </binding>
        </textfield>

        <textfield label="surname">
            <binding variable="lm">
                <attribute name="surname"/>
            </binding>
        </textfield>

        <textfield label="mobileNumber">
            <binding variable="lm">
                <attribute name="mobileNumber"/>
            </binding>
        </textfield>

        <submit label="invite" action="inviteLM"/>

    </view>
</ui>





