unit ProgramManagerEdit;

ProgramManager programManager;

bool isEdit;

string nav_return;

string message;

void init (){

	if (programManager == null){
		programManager = ProgramManager:new();
		isEdit = false;
	} else {
		isEdit = true;
	}
	
	if (nav_return == null){
		nav_return = "nav_program_manager_management";
	}	
}

string submitInvite(){
    //programManager.role_id = 1;
    programManager.registered = Mez:now();
    programManager.save();

    if (isEdit == false){
	    programManager.invite(programManager.mobileNumber);
    }
    
    message = "Learning Manager saved";
    Alerter:alert(message);
    
    nav_return = "nav_program_manager_management";
    return navigate();
}


string back(){
	nav_return = "nav_program_manager_management";
	return navigate();
}

string navigate(){
	resetVars();
	return nav_return;
}

void resetVars(){
	programManager = null;
}