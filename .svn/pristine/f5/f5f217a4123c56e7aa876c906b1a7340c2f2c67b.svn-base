<?xml version="1.0" encoding="UTF-8"?>
<ui xmlns="http://uiprogram.mezzanine.com/View" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://uiprogram.mezzanine.com/View ../../xsd/View.xsd">
    <view label="learning_manager_management.title" unit="LearningManagerEdit" init="init">

        <navigation outcome="back" target="SystemOverview" />
        <navigation outcome="nav_learning_manager_management" target="LearningManagerManagement" />

        <action label="back" action="backButton"/>

        <textfield label="firstNames">
            <binding variable="learningManager">
                <attribute name="firstNames"/>
            </binding>
        </textfield>

        <textfield label="surname">
            <binding variable="learningManager">
                <attribute name="surname"/>
            </binding>
        </textfield>

        <textfield label="mobileNumber">
            <binding variable="learningManager">
                <attribute name="mobileNumber"/>
            </binding>
        </textfield>
 
        <submit label="submit" action="submitInvite"/>

    </view>
</ui>