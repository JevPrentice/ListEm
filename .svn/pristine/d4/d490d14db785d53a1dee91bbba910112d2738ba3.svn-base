@Restrict("SmartChew", all())
@Restrict("FieldWorker", all()) /*NY: added 2014/08/11 */
persistent object ClientGroup{
	string   idd; // _ID; 
    @RequiredAttribute("clientGroup.requirefield")
	string name;
	
	/* JP 2015-03-23 #355 */
	GEOGRAPHY geographyType;
	/* JP 2015-03-23 #355 */

	/* 
	 * Each group has many CHW's [Client with roleID=1] assigned 
	 * Each Client can only be assigned to one group [regardless of roleID=1 or roleID=2]
	*/
	@OneToMany
	Client client_chws via group;

	/* 
	 * Each group has one CHEW [Client with roleID=2] assigned
	 * A CHEW [Client with roleID=2 can be the 'CHEW' of many groups
	*/
 	@ManyToOne
	Client client_chews;

	/* JP 2015-03-23 #355 */
	@ManyToOne
	SubCounty subCounty;
	/* JP 2015-03-23 #355 */

}
