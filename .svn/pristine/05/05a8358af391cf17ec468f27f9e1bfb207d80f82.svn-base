@Restrict("SmartChew", all())
@Restrict("FieldWorker", all()) /*NY: added 2014/08/11 */
persistent object PatternHistory {
	
	datetime startDate;
	datetime maxEndDate;
	datetime reminderTime;
	datetime escalationTime;
	int statusID;
	int escalationID;
	datetime actualEndDate;

	@ManyToOne
	Client client_patternHistory;
	
	@ManyToOne
	Pattern pattern_patternHistory;
	
	@ManyToOne
	Sequence sequence_patternHistory;
	
}