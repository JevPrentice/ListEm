<?xml version="1.0" encoding="UTF-8"?>
<ui xmlns="http://uiprogram.mezzanine.com/View" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://uiprogram.mezzanine.com/View ../../xsd/View.xsd">
    <view label="ManageSubCounties.title" unit="ManageSubCounties" init="init">
    
        <navigation outcome="navManageCounties" target="ManageCounties"/>
        <navigation outcome="navEditSubCounty" target="EditSubCounty"/>

        <action label="back" action="back"/>
        <action label="ManageCounties.navCreateSubCounty" action="navCreateSubCounty"/>

        <info label="County.countyName">
            <binding variable="county">
                <attribute name="countyName"/>
            </binding>
        </info>

        <table title="ManageSubCounties.allSubCounties">
                <collectionSource variable="subCounties"/>
                <column heading="SubCounty.subCountyName">
                    <attributeName>subCountyName</attributeName>
                </column>
                <column heading="County.countyName">
                    <attributeName>county.countyName</attributeName>
                </column>
                <rowAction label="edit" action="editSubCounty">
                    <binding variable="subCounty"/>
                </rowAction>
                <rowAction label="delete" action="deleteCounty">
                    <binding variable="subCounty"/>
                    <confirm subject="ManageSubCounties.confirmDelete.subject" body="ManageSubCounties.confirmDelete.body"/>
                </rowAction>
        </table>

    </view>
</ui> 

