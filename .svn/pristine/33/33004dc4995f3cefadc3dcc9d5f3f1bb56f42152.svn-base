unit PatternPPEList;

PatternPPE patternPPE;

void init() {
	patternPPE = PatternPPE:new();
}

string savePatternPPE() {
	patternPPE.save();
	return null;
}

string editPatternPPE() {
	return null;
}

string deletePatternPPE() {
	PatternPPE:delete(patternPPE);
	return null;
}

PatternPPE [] getAllPatternPPEs() {
	return PatternPPE:all();
}

void updatePatternPPE(Client client, Pattern pattern) {
	
	PatternPPE patternPPE = pattern.pattern_patternPPE;
	patternPPE.progress = 100;
	patternPPE.save();
}