@Restrict("SmartChew", all())
persistent object ClientDetails {
	
	string training_mode;
	string type_of_phone;
	string platform_type;
	string mobile_service_provider;
	string education_level;
	string years_at_chv;
	string email_address;

	date date_of_birth;

	@OneToOne
	Client client via client_details;
	
}