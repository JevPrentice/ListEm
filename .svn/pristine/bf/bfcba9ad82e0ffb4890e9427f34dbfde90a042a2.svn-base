<?xml version="1.0" encoding="UTF-8"?>
<ui xmlns="http://uiprogram.mezzanine.com/View" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://uiprogram.mezzanine.com/View ../../xsd/View.xsd">
    <view label="cu_perfomance.title" unit="Laggards" init="init">

        <navigation outcome="navCUPerformance" target="CUPerformance" />
        <action label="back" action="back"/>

        <table>
            <collectionSource variable="laggards"/>
            <column heading="firstName">
                <attributeName>firstName</attributeName>
            </column>
            <column heading="surname">
                <attributeName>surname</attributeName>
            </column>
            <column heading="mobileNumber">
                <attributeName>mobileNumber</attributeName>
            </column>
            <rowAction label="send_sms" action="sendReminder">
                <binding variable="selectedLaggard"/>
            </rowAction>
        </table>
    </view>
</ui>