<?xml version="1.0" encoding="UTF-8"?>
<ui xmlns="http://uiprogram.mezzanine.com/View" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://uiprogram.mezzanine.com/View ../../xsd/View.xsd">
    <view label="jumpto.page.title.messagetext" unit="JumpToList" init="initmessagetext">

    <navigation outcome="back" target="vJumpToMessages" />
    <action label="jumpto.page.back" outcome="back"/>

<!-- ********************************************************************* -->
<!-- ********************************************************************* -->

       <info label="jumpto.messageIn_JumpTo">
            <binding variable="jt_messageIn_JumpTo">
                <attribute name="_MTID"/>
            </binding>
        </info>

        <textarea label="jumpto.viewJumpToMessageTextIn">
            <binding variable="messageTextIn">
            </binding>
        </textarea>

        <info label="jumpto.userResponse">
            <binding variable="jtViewText">
                <attribute name="userResponse"/>
            </binding>
        </info>
        
        <info label="jumpto.messageTo_JumpTo">
            <binding variable="jt_messageTo_JumpTo">
                <attribute name="_MTID"/>
            </binding>
        </info>

        <textarea label="jumpto.viewJumpToMessageTextOut">
            <binding variable="messageTextOut">
            </binding>
        </textarea>

        <info label="jumpto.scoreAdjustment">
            <binding variable="jtViewText">
                <attribute name="scoreAdjustment"/>
            </binding>
        </info>

    </view>
</ui>