<?xml version="1.0" encoding="UTF-8"?>
<ui xmlns="http://uiprogram.mezzanine.com/View" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://uiprogram.mezzanine.com/View ../../xsd/View.xsd">
    <view label="user_activity.title" unit="UserActivityOutgoingSMS" init="init">

        <navigation outcome="nav_user_activity" target="UserActivity" />

        <action label="back" action="back"/>

        <submit label="user_profile.refresh_performance" action="init"/>
        <submit label="user_profile.showalloutgoingsms" action="bShowAlloutgoingSMSLog"/>

        <info label="client.name">
            <binding variable="vclientname">
            </binding>
        </info>

        <info label="client.currentactivity">
            <binding variable="vcurrentactivity">
            </binding>
        </info>

        <table title="user_activity.umbrellaPPE.title">
            <collectionSource variable="uPArr"/>
            <column heading="user_activity.umbrellaPPE.umbrellaStartDateTime">
                <attributeName>umbrellaStartDateTime</attributeName>
            </column>
            <column heading="user_activity.umbrellaPPE.umbrella_umbrellaPPE">
                <attributeName>umbrella_umbrellaPPE.umbrellaID</attributeName>
            </column>
            <column heading="user_activity.umbrellaPPE.umbrellaEscalateDateTime">
                <attributeName>umbrellaEscalateDateTime</attributeName>
            </column>
            <column heading="user_activity.umbrellaPPE.umbrellaEndDateTime">
                <attributeName>umbrellaEndDateTime</attributeName>
            </column>
            <column heading="user_activity.umbrellaPPE.durationhours">
                <attributeName>durationhours</attributeName>
            </column>
            <column heading="user_activity.umbrellaPPE.status">
                <attributeName>status</attributeName>
            </column>
            <column heading="user_activity.umbrellaPPE.progress">
                <attributeName>progress</attributeName>
            </column>

            <column heading="user_activity.umbrellaPPE.chatscore">
                <attributeName>chatscore</attributeName>
            </column>
            <column heading="user_activity.umbrellaPPE.chatscoreadjusted">
                <attributeName>chatscoreadjusted</attributeName>
            </column>

            <column heading="user_activity.umbrellaPPE.quizpercentage">
                <attributeName>quizpercentage</attributeName>
            </column>
            <column heading="user_activity.umbrellaPPE.quizscore">
                <attributeName>quizscore</attributeName>
            </column>

            <column heading="user_activity.umbrellaPPE.practicumtotalscoreachieved">
                <attributeName>practicumtotalscoreachieved</attributeName>
            </column>
            <column heading="user_activity.umbrellaPPE.practicumpercentage">
                <attributeName>practicumpercentage</attributeName>
            </column>
            <column heading="user_activity.umbrellaPPE.practicumscore">
                <attributeName>practicumscore</attributeName>
            </column>

            <column heading="user_activity.umbrellaPPE.totalscore">
                <attributeName>totalscore</attributeName>
            </column>
            <rowAction label="user_activity.umbrellaPPE.delete" action="fDeleteUserUmbrellaPPE">
                <binding variable="uPdelete"/>
                <confirm subject="user_activity.umbrellaPPE.confirmationsubject" body="user_activity.umbrellaPPE.confirmation"/>
            </rowAction>
        </table>

        <info label="infolabel">
            <binding variable="vinfonote">
            </binding>
        </info>

        <table title="learningManager.outgoingSMSList.list.title">
            <collectionSource variable="outgoingSmsArr"/>
            <column heading="outgoingSMS.dateTimeStamp">
                <attributeName>dateTimeStamp</attributeName>
            </column>
             <column heading="topic.topicID">
                <attributeName>umbrella_outgoingSMSLog.umbrellaID</attributeName>
            </column>
             <column heading="sub_topic.subTopicID">
                <attributeName>topic_outgoingSMSLog.topicID</attributeName>
            </column>
             <column heading="activity.activityID">
                <attributeName>pattern_outgoingSMSLog.patternID</attributeName>
            </column>
            <column heading="message.messageID">
                <attributeName>message_outgoingSMSLog._MTID</attributeName>
            </column>
            <column heading="outgoingSMS.messageText">
                <attributeName>messageText</attributeName>
            </column>
            <column heading="outgoingSMS.validFirstLetter">
                <attributeName>validFirstLetter</attributeName>
            </column>
            <column heading="outgoingSMS.correctAnswer">
                <attributeName>correctAnswer</attributeName>
            </column>
            <column heading="outgoingSMS.responseText">
                <attributeName>responseText</attributeName>
            </column>
            <column heading="outgoingSMS.status">
                <attributeName>status</attributeName>
            </column>
            <column heading="outgoingSMS.userScore">
                <attributeName>userScore</attributeName>
            </column>            
        </table>

        <table title="learningManager.TopicHistory.TopicLogTableTitle">
            <collectionSource function="fGetTopicHistoryLogFiltered"/>
            <column heading="topic">
                <attributeName>umbrella_topicHistoryLog.umbrellaID</attributeName>
            </column>
            <column heading="learningManager.TopicHistory.Topic">
                <attributeName>topic_topicHistoryLog.topicID</attributeName>
            </column>
            <column heading="learningManager.TopicHistory.Client">
                <attributeName>client_topicHistoryLog.mobileNumber</attributeName>
                <attributeName>client_topicHistoryLog.firstName</attributeName>                
            </column>
            <column heading="learningManager.TopicHistory.dateTimeStamp">
                <attributeName>dateTimeStamp</attributeName>
            </column>
            <column heading="learningManager.TopicHistory.statusID">
                <attributeName>statusID</attributeName>
            </column>
            <column heading="learningManager.TopicHistory.runType">
                <attributeName>runType</attributeName>
            </column>
            <column heading="learningManager.TopicHistory.topicAttempt">
                <attributeName>topicAttempt</attributeName>
            </column>
            <column heading="learningManager.TopicHistory.userScore">
                <attributeName>userScore</attributeName>
            </column>

        </table>

        <table title="learningManager.PPE.topictitle">
            <collectionSource variable="tPPEs"/>
            <column heading="topic">
                <attributeName>umbrella_topicPPE.umbrellaID</attributeName>
            </column>
            <column heading="PPE.topic">
                <attributeName>topic_topicPPE.topicID</attributeName>
            </column>
            <column heading="PPE.progress">
                <attributeName>progress</attributeName>
            </column>
            <column heading="PPE.effort">
                <attributeName>effort</attributeName>
            </column>
            <column heading="PPE.score">
                <attributeName>score</attributeName>
            </column>
            <column heading="PPE.performance">
                <attributeName>performance</attributeName>
            </column>
        </table>

        <table title="learningManager.PPE.sequencetitle">
            <collectionSource variable="sPPEs"/>
            <column heading="PPE.sequence">
                <attributeName>sequence_sequencePPE.sequenceID</attributeName>
            </column>
            <column heading="PPE.progress">
                <attributeName>progress</attributeName>
            </column>
            <column heading="PPE.effort">
                <attributeName>effort</attributeName>
            </column>
            <column heading="PPE.score">
                <attributeName>score</attributeName>
            </column>            
            <column heading="PPE.performance">
                <attributeName>performance</attributeName>
            </column>
        </table>

        <table title="learningManager.PPE.patterntitle">
            <collectionSource variable="pPPEs"/>
            <column heading="PPE.pattern">
                <attributeName>pattern_patternPPE.patternID</attributeName>
            </column>
            <column heading="PPE.progress">
                <attributeName>progress</attributeName>
            </column>
            <column heading="PPE.effort">
                <attributeName>effort</attributeName>
            </column>
            <column heading="PPE.score">
                <attributeName>score</attributeName>
            </column>            
            <column heading="PPE.performance">
                <attributeName>performance</attributeName>
            </column>
        </table>

    </view>
</ui>