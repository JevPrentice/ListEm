unit SequenceOrderList;

SequenceOrder sequenceOrder;
SequenceOrder deleteSequenceOrder;

Topic topic;
Sequence sequence;

void init() {
	sequenceOrder = SequenceOrder:new();
}

string editSequenceOrder() {
    sequence = sequenceOrder.sequence_sequenceOrder;
    topic = sequenceOrder.topic_sequenceOrder;
	return null;
}
string saveSequenceOrder() {
	sequenceOrder.topic_sequenceOrder = topic;
	sequenceOrder.sequence_sequenceOrder = sequence;
	sequenceOrder.save();
    init();
	return null;
}

string deleteSequenceOrder() {
	SequenceOrder:delete(deleteSequenceOrder);
	return null;
}

SequenceOrder[] getAllSequenceOrders() {
	return SequenceOrder:all();
}
