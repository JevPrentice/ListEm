unit OverallCUPerformanceFilter;

Umbrella[] topics;

Umbrella selectedTopic;
datetime dStart;
datetime dEnd;

void init(){

	SmartChew chew = SmartChew:user();
	Client c = chew.client;

	topics = TopicController:getTopicsWithActiveAndScheduledChvsForChew(c);
}

string navigate(string location){
	return location;
}

string back(){
	return navigate("navCUPerformance");
}

string viewResults(){

	if (selectedTopic == null){
		Alerter:alert("No Topic selected");
		return null;
	}

	OverallCUPerformanceResults:selectedTopic = selectedTopic;
	OverallCUPerformanceResults:dStart = dStart;
	OverallCUPerformanceResults:dEnd = dEnd;
	return navigate("navOverallCUPerformanceResults");
}

