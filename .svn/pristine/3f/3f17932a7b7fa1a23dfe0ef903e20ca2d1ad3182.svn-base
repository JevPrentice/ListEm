<?xml version="1.0" encoding="UTF-8"?>
<ui xmlns="http://uiprogram.mezzanine.com/View" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://uiprogram.mezzanine.com/View ../views/xsd/View.xsd">
	<view label="user_bulk_upload.title" unit="UserBulkUpload" init="init" >

        <navigation outcome="nav_user_management" target="UserManagement"/>

        <action label="back" action="back"/>
 
        <info label="user_bulk_upload.csv_template_header">
            <binding variable="csv_template_header">
            </binding>
        </info>

         <info label="user_bulk_upload.csv_template_content">
            <binding variable="csv_template_content">
            </binding>
        </info>

        <select label="user_management.group_name">
            <binding variable="uClientGroup"></binding>
            <collectionSource function="UserEdit:getAllClientGroups">
                <displayAttribute name="name"/>
            </collectionSource>
        </select>

        <fileupload label="user_bulk_upload.fileupload">
            <binding variable="uploadedFile">
                <attribute name="data" />
            </binding>
        </fileupload>
       
        <submit label="save" action="processCSV"/>

	</view>
</ui>