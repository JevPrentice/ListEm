@Restrict("SmartChew", all())
@Restrict("Pharmacist", all()) /*NY: added 2014/08/11 */
@Restrict("FieldWorker", all()) /*NY: added 2014/08/11 */
persistent object InputTypeObject{
	
    string messageID; 
	string text;
	string version;
	string status;

	blob blobValue;
	int numericValue;
	datetime datetimeValue;	
	date dateValue;	

	datetime timestamp;
