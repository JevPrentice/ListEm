<?xml version="1.0" encoding="UTF-8"?>
<ui xmlns="http://uiprogram.mezzanine.com/View" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://uiprogram.mezzanine.com/View ../../xsd/View.xsd">
    <view label="jumpto.page.title.all" unit="JumpToList" init="init">

<!--     <menuitem label="jumpto.menu.title" icon="Users" order="10">
        <userRole>LearningManager</userRole>
        <userRole>ProgramManager</userRole>
    </menuitem>
 -->

    <navigation outcome="vJumpToMessages" target="vJumpToMessages" />
    <action label="jumpto.vJumpToMessages" outcome="vJumpToMessages"/>
    <navigation outcome="vJumpToTopics" target="vJumpToTopics" />
    <action label="jumpto.vJumpToTopics" outcome="vJumpToTopics"/>

<!-- ********************************************************************* -->
<!-- ********************************************************************* -->

        <info label="jumpto.note">
            <binding variable="sjt_activityfilter_JumpTo">
            </binding>
        </info>

        <select label="jumpto.activityfilter_JumpTo">
            <binding variable="jt_activityfilter_JumpTo"/>
           <collectionSource variable="vPs">
                <displayAttribute name="patternID"/>
            </collectionSource>
        </select>

        <submit label="jumpto.filter" action="fFilterJumpToMessage"/>
        
<!-- ********************************************************************* -->
<!-- ********************************************************************* -->

        <info label="jumpto.note">
            <binding variable="sjt_userResponse">
            </binding>
        </info>

        <textfield label="jumpto.userResponse">
            <binding variable="jt">
                <attribute name="userResponse"/>
            </binding>
        </textfield>

<!-- ********************************************************************* -->

        <info label="jumpto.note">
            <binding variable="sjt_messageIn_JumpTo">
            </binding>
        </info>

        <select label="jumpto.messageIn_JumpTo">
            <binding variable="jt_messageIn_JumpTo"/>
           <collectionSource variable="vMs">
                <displayAttribute name="_MTID"/>
            </collectionSource>
        </select>

<!-- ********************************************************************* -->

        <info label="jumpto.note">
            <binding variable="sjt_umbrella_JumpTo">
            </binding>
        </info>

        <select label="jumpto.umbrella_JumpTo">
            <binding variable="jt_umbrella_JumpTo"/>
           <collectionSource variable="vUs">
                <displayAttribute name="umbrellaID"/>
            </collectionSource>
        </select>

<!-- ********************************************************************* -->

        <info label="jumpto.note">
            <binding variable="sjt_topic_JumpTo">
            </binding>
        </info>

        <select label="jumpto.topic_JumpTo">
            <binding variable="jt_topic_JumpTo"/>
           <collectionSource variable="vTs">
                <displayAttribute name="topicID"/>
            </collectionSource>
        </select>

<!-- ********************************************************************* -->

        <info label="jumpto.note">
            <binding variable="sjt_messageTo_JumpTo">
            </binding>
        </info>

        <select label="jumpto.messageTo_JumpTo">
            <binding variable="jt_messageTo_JumpTo"/>
           <collectionSource variable="vMs">
                <displayAttribute name="_MTID"/>
            </collectionSource>
        </select>

<!-- ********************************************************************* -->

        <info label="jumpto.note">
            <binding variable="sjt_jumpToType">
            </binding>
        </info>
        <textfield label="jumpto.jumpToType">
            <binding variable="jt">
                <attribute name="jumpToType"/>
            </binding>
        </textfield>

<!-- ********************************************************************* -->

        <info label="jumpto.note">
            <binding variable="sjt_scoreAdjustment">
            </binding>
        </info>

        <textfield label="jumpto.scoreAdjustment">
            <binding variable="jt">
                <attribute name="scoreAdjustment"/>
            </binding>
        </textfield>

<!-- ********************************************************************* -->

        <submit label="jumpto.save" action="saveJumpTo"/>

<!-- ********************************************************************* -->
<!-- ********************************************************************* -->


    <table title="jumpto.table.title">
        <collectionSource variable="vJs"/>

        <column heading="jumpto.messageIn_JumpTo">
            <attributeName>messageIn_JumpTo._MTID</attributeName>
        </column>
        <column heading="jumpto.userResponse">
            <attributeName>userResponse</attributeName>
        </column>
        <column heading="jumpto.umbrella_JumpTo">
            <attributeName>umbrella_JumpTo.umbrellaID</attributeName>
        </column>
        <column heading="jumpto.topic_JumpTo">
            <attributeName>topic_JumpTo.topicID</attributeName>
        </column>
        <column heading="jumpto.messageTo_JumpTo">
            <attributeName>messageTo_JumpTo._MTID</attributeName>
        </column>
        <column heading="jumpto.scoreAdjustment">
            <attributeName>scoreAdjustment</attributeName>
        </column>
        <column heading="jumpto.jumpToType">
            <attributeName>jumpToType</attributeName>
        </column>
        <rowAction label="jumpto.edit" action="editJumpTo">
            <binding variable="jt"/>
        </rowAction>
        <rowAction label="delete" action="deleteJumpTo">
            <binding variable="jt"/>
        </rowAction>

    </table>

<!-- ********************************************************************* -->
<!-- ********************************************************************* -->

        <textarea label="configHelp.view">
            <binding variable="configHelp">
            </binding>
        </textarea>
        <submit label="jumpto.save" action="saveConfig"/>

    </view>
</ui>