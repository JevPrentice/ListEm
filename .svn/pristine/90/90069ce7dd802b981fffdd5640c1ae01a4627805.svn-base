<?xml version="1.0" encoding="UTF-8"?>
<ui xmlns="http://uiprogram.mezzanine.com/View" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://uiprogram.mezzanine.com/View ../../xsd/View.xsd">
    <view label="sequence_create_edit.title" unit="SequenceEdit" init="init">

        <navigation outcome="nav_sequence_management" target="SequenceManagement"/>
        <navigation outcome="nav_content_viewer" target="ContentViewer"/>
        <navigation outcome="nav_sequence_order_edit" target="SequenceOrderEdit"/>
        <navigation outcome="nav_sequence_order_management" target="SequenceOrderManagement"/>

        <action label="back" action="navigate"/>

        <textfield label="sequence_management.sequenceID">
            <binding variable="sequenceID" />
        </textfield>

        <textfield label="sequence_management.sequenceName">
            <binding variable="sequence">
                <attribute name="sequenceName"/>
            </binding>
        </textfield>

        <textfield label="sequence_management.sequenceStartSMS">
            <binding variable="sequence">
                <attribute name="sequenceStartSMS"/>
            </binding>
        </textfield>

        <textfield label="sequence_management.sequenceEndSMS">
            <binding variable="sequence">
                <attribute name="sequenceEndSMS"/>
            </binding>
        </textfield>

        <textfield label="sequence_management.sequenceReminderSMS">
            <binding variable="sequence">
                <attribute name="sequenceReminderSMS"/>
            </binding>
        </textfield>

        <textfield label="sequence_management.sequenceReminderTime">
            <binding variable="sequence">
                <attribute name="sequenceReminderTime"/>
            </binding>
        </textfield>

        <submit label="save" action="saveSequenceButton"/>
        <submit label="sequence_create_edit.save_nav_sequence_order" action="saveSequenceNavSequenceOrderEdit"/>

    </view>
</ui>